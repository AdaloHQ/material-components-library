{
  "displayName": "Icon",
  "icon": "icon.png",
  "defaultWidth": 24,
  "defaultHeight": 24,
  "component": "./index.js",
  "resizeX": false,
  "resizeY": false,
  "props": [
    {
      "name": "iconName",
      "displayName": "Icon",
      "type": "icon",
      "default": "chevron-right"
    },
    {
      "name": "iconColor",
      "displayName": "Color",
      "type": "color",
      "default": "#666666"
    },
    {
      "name": "iconSize",
      "displayName": "Size",
      "type": "number",
      "default": 24,
      "control": { "type": "slider", "max": 70, "min": 1 }
    },
    {
      "name": "onPress",
      "type": "action",
      "displayName": "Click Actions"
    }
  ]
}
