        }
      ]
    },
    {
      "name": "icon1",
      "displayName": "First Icon",
      "role": "listItem",
      "reference": "items",
      "props": [
        {
          "name": "enabled",
          "type": "boolean",
          "default": false
        },
        {
          "name": "iconType",
          "displayName": "Icon Type",
          "type": "text",
          "default": "icon",
          "control": {
            "type": "menu",
            "options": [
              { "label": "Icon", "value": "icon" },
              { "label": "Toggle", "value": "toggle" }
            ]
          }
        },
        {
          "name": "icon",
          "type": "icon",
          "default": "favorite",
          "enabled": {
            "iconType": ["toggle", "ADALO_INTERNAL_!previous"]
          }
        },
        {
          "name": "onPress",
          "displayName": "Click Actions",
          "type": "action",
          "enabled": {
            "iconType": ["toggle", "ADALO_INTERNAL_!previous"]
          }
        },
