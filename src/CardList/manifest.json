          "type": "boolean",
          "default": true
        },
        {
          "name": "backgroundColor",
          "displayName": "Background Color",
          "type": "color",
          "default": "@primary",
          "enabled": {
            "background": true
          }
        },
        {
          "name": "border",
          "displayName": "Button Border",
          "type": "boolean",
          "default": false
        },
        {
          "name": "borderColor",
          "displayName": "Border Color",
          "type": "color",
          "default": "@primary",
          "enabled": {
            "border": true
          }
        },
        {
          "name": "borderSize",
